#include <iostream>
using namespace std;
int main()
{
    int n,digit,number=0,b;
    cin>>n;
    b=n;
    while (n!=0)
    {
        digit=n%10;
        number=number*10+digit;
         n=n/10;
    }
    cout<<number<<endl;
    if(b==number)
    {
        cout<<"It is the palindrome";
    }
    else
    {
        cout<<"Not a palindrome";
    }
}
